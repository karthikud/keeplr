<html ng-app="keeplr">
<head>
<title>Keeplr</title>
<link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.css">
<link href="https://fonts.googleapis.com/icon?family=Material+Icons"
rel="stylesheet">

</head>
<body  ng-controller="AppCtrl" ng-cloak>

<md-sidenav layout="column" class="md-sidenav-left md-whiteframe-1dp" md-component-id="left" >
<md-toolbar class="md-tall md-hue-2">
<span flex></span>
<div layout="column" class="md-toolbar-tools-bottom inset">
<md-list-item>
<img ng-src="<%= user.google.image %>" class="md-avatar" />
</md-list-item>
<span></span>
<div><%= user.google.name %></div>
<div><%= user.google.email %></div>

</div>
</md-toolbar>
<md-list>
<md-list-item ng-repeat="item in menu">
<md-button href="{{item.link}}">{{item.title}}</md-button>
</md-list-item>
<md-divider></md-divider>
<md-subheader>Management</md-subheader>
<md-list-item ng-repeat="item in admin">
<md-button href="{{item.link}}">{{item.title}}</md-button>

</md-list-item>
</md-list>
</md-sidenav>
 

<div layout="column" class="relative" layout-fill role="main">

<md-toolbar>
<div class="md-toolbar-tools">

<md-button ng-click="toggleSidenav('left')"  aria-label="Menu">
<ng-md-icon icon="menu"></ng-md-icon>
</md-button>
<h3>
Dashboard
</h3>
<span flex></span>
<md-button aria-label="Search">
<ng-md-icon icon="search"></ng-md-icon>
</md-button>
<md-button aria-label="Open Settings" ng-click="showListBottomSheet($event)">
<ng-md-icon icon="more_vert"></ng-md-icon>
</md-button>
</div>

</md-toolbar>

<div layout="row">
<md-button ng-click="showAdd($event)" class="md-fab md-primary">
<i class="material-icons">add</i>
</md-button>
<md-button ng-click="showAddcategory($event)" class="md-fab md-primary">
<i class="material-icons">filter_none</i>
</md-button>
</div>

<div layout="row" flex>
<div class='md-padding' layout="row" layout-wrap flex > <md-card style="width: 200px;" ng-repeat="item in categories">

<md-card-header>
<md-card-avatar>
<img class="md-user-avatar" src="<%= user.google.image %>"/>
</md-card-avatar>
<md-card-header-text>
<span class="md-title"><%= user.google.name %></span>
<span class="md-subhead">Belongsto</span>
</md-card-header-text>
</md-card-header>
<md-card-content>

<h3 class="md-headline">{{item.name}}</h3>

</md-card-content>
<div class="md-actions" layout="row" layout-align="end center">
<md-button ng-click="deleteboard(item._id)"><i class="material-icons">delete_sweep</i></md-button>
<md-button ng-click="showboard(item._id)">View</md-button>
</div>
</md-card></div>
<div layout="column" flex>
<div class='md-padding' layout="row" ><md-card style="width: 200px;" ng-repeat="item in bookmarks">

<md-card-header>
<md-card-avatar>
<img class="md-user-avatar" src="<%= user.google.image %>">

</md-card-avatar>
<md-card-header-text>
<span class="md-title"><%= user.google.name %></span>
<span class="md-subhead">Belongsto</span>
</md-card-header-text>
</md-card-header>
<img ng-src="card-image.png" class="md-card-image" alt="image caption">
<md-card-content>

<h3 class="md-subhead">{{item.url}}</h3>

</md-card-content>
<div class="md-actions" layout="row" layout-align="end center">
<md-button ng-click="deletebookmark(item._id)"><i class="material-icons">delete_sweep</i></md-button>
<md-button href="{{item.url}}" target="_blank"><i class="material-icons">open_in_new</i></md-button>

</div>
</md-card>
</div>
</div></div>



</div>
</div>







</div>



<!-- Angular Material Dependencies -->
<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>
<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-route.min.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.min.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-aria.min.js"></script>

<script src="//ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.js"></script>

<script src="//cdn.jsdelivr.net/angular-material-icons/0.4.0/angular-material-icons.min.js"></script> 
<script src="/js/material.js"></script> 
<script src="/js/services/todos.js"></script>
<script src="/js/services/category.js"></script> 
<script src="/js/services/user.js"></script> 
</body>
</html>
